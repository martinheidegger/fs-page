{
  "name": "fs-page",
  "version": "1.1.0",
  "description": "Flexible processor of files. Provides extra information taken from the path and front-matter and other tools.",
  "main": "index.js",
  "scripts": {
    "tap": "tap --reporter=spec --check-coverage test/*.js",
    "quick": "tap test/*.js",
    "test": "standard && npm run tap"
  },
  "keywords": [
    "static-site-generator",
    "processor",
    "front-matter",
    "parser",
    "compiler"
  ],
  "bin": {
    "fs-page": "bin/fs-page"
  },
  "author": "Martin Heidegger <martin.heidegger@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "coveralls": "^2.11.8",
    "standard": "^6.0.8",
    "tap": "^5.7.0"
  },
  "dependencies": {
    "cheerio": "^0.20.0",
    "excerpt-html": "^1.0.1",
    "front-matter": "^2.0.6",
    "fs-date-parser": "^1.0.0",
    "good-thumbs": "^1.6.0",
    "markdown-it": "^6.0.0",
    "minimist": "^1.2.0",
    "slug": "^0.9.1",
    "through2": "^2.0.1"
  }
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/martinheidegger/fs-page.git"
  },
  "bugs": {
    "url": "https://github.com/martinheidegger/fs-page/issues"
  },
  "homepage": "https://github.com/martinheidegger/fs-page#readme"
}
